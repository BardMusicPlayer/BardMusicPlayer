<Application x:Class="BardMusicPlayer.Ui.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BardMusicPlayer.Ui"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <!-- Playbar images -->
        <ImageBrush x:Key="MAIN_POSITION_SLIDER_THUMB"></ImageBrush>
        <ImageBrush x:Key="MAIN_POSITION_SLIDER_THUMB_SELECTED"></ImageBrush>
        <ImageBrush x:Key="MAIN_VOLUME_THUMB"></ImageBrush>
        <ImageBrush x:Key="MAIN_VOLUME_THUMB_SELECTED"></ImageBrush>
        <ImageBrush x:Key="MAIN_BALANCE_THUMB"></ImageBrush>
        <ImageBrush x:Key="MAIN_BALANCE_THUMB_SELECTED"></ImageBrush>
        <ImageBrush x:Key="PLAYLIST_SLIDER_THUMB"></ImageBrush>
        <ImageBrush x:Key="PLAYLIST_SLIDER_THUMB_SELECTED"></ImageBrush>
        <ImageBrush x:Key="MAIN_VOLUME_BACKGROUND"></ImageBrush>
    </Application.Resources>
</Application>
