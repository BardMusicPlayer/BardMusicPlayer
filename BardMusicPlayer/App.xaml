<Application x:Class="BardMusicPlayer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <!-- Playbar images -->
        <ImageBrush x:Key="MAIN_POSITION_SLIDER_THUMB" />
        <ImageBrush x:Key="MAIN_POSITION_SLIDER_THUMB_SELECTED" />
        <ImageBrush x:Key="MAIN_VOLUME_THUMB" />
        <ImageBrush x:Key="MAIN_VOLUME_THUMB_SELECTED" />
        <ImageBrush x:Key="MAIN_BALANCE_THUMB" />
        <ImageBrush x:Key="MAIN_BALANCE_THUMB_SELECTED" />
        <ImageBrush x:Key="PLAYLIST_SLIDER_THUMB" />
        <ImageBrush x:Key="PLAYLIST_SLIDER_THUMB_SELECTED" />
        <ImageBrush x:Key="MAIN_VOLUME_BACKGROUND" />
    </Application.Resources>
</Application>